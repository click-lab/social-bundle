{
  "name" : "clicklab/socialbundle",
  "description" : "Social Bundle",
  "type" : "symfony-bundle",
  "authors" : [{
    "name" : "sacha",
    "email" : "sacha@click-eat.fr"
  }],
  "keywords" : [
    "social bundle"
  ],
  "license" : [
    "MIT"
  ],
  "require" : {
    "gedmo/doctrine-extensions":"2.4.*",
    "vich/uploader-bundle": "dev-master",
    "opauth/opauth": "1.0.*@dev",
    "opauth/facebook": "1.0.*@dev",
    "opauth/twitter": "1.0.*@dev",
    "liip/imagine-bundle": "^1.3",
    "clicklab/restaurantbundle":"^1.0",
    "symfony/symfony": "2.7.*",
    "doctrine/orm": "^2.4.8",
    "doctrine/doctrine-bundle": "~1.4"
  },
  "autoload" : {
    "psr-0" : {
      "Clab\\SocialBundle" : ""
    }
  },
  "target-dir" : "Clab/SocialBundle",
  "repositories" : [{
     "type": "vcs", "url": "https://github.com/click-lab/social-bundle"
  }],
  "extra" : {
    "branch-alias" : {
      "dev-master" : "master"
    }
  }
